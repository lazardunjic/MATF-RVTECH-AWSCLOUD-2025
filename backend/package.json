{
  "name": "charging-map",
  "version": "1.0.0",
  "description": "Serverless EV Charging Map with LocalStack",
  "main": "index.js",
  "scripts": {
    "deploy": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "invoke:sync": "serverless invoke -f syncOCMdata --stage dev --log",
    "invoke:get": "serverless invoke -f getChargers --stage dev --log",
    "remove": "serverless remove --stage dev",
    "logs": "serverless logs -f getChargers --stage dev"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "localstack",
    "ev-charging"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1691.0",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "serverless": "^3.40.0",
    "serverless-localstack": "^1.3.1"
  }
}
